<Window x:Class="WpfClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfClient"
        xmlns:view="clr-namespace:WpfClient.View"
        xmlns:window="clr-namespace:WpfClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="800">
    <Grid>
        <TabControl x:Name="TabControl">
            <TabItem Header="GetValues">
                <view:GetValuesView x:Name="GetValuesView"/>
            </TabItem>
            <TabItem Header="GetExtentValues">
                <view:GetExtentValuesView x:Name="GetExtentValuesView"/>
            </TabItem>
            <TabItem Header="GetRealtedValues">
                <view:GetRelatedValuesView x:Name="GetRelatedValuesView"/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
